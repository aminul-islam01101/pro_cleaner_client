{
  "name": "next-redux-ant-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",

    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx",
    "prettier:check": "prettier --ignore-path .gitignore --write \"src/app/**/*.+(js|ts|tsx|json)\"",
  
    "prettier:fix": "prettier --write \"src/app/**/*.+(js|ts|tsx|json)\"",
    "lint-prettier-fix": " npm run prettier:fix  && npm run lint",
    "lint-prettier-check": "npm run lint:check && npm run prettier:check",
    "lint-prettier": "npm run lint-prettier-fix && npm run lint-prettier-check",
    "lint-staged": "npm run  lint-prettier"
  },
  "lint-staged": {
    "src/app/**/*.{ts,tsx,js}": "npm run lint-prettier"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.0",
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "^5.10.0",
    "axios": "^1.5.1",
    "jwt-decode": "^3.1.2",
    "next": "13.5.4",
    "rc-slider": "^10.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.46.2",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
   
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-pretty": "^5.0.0",
    
    "eslint-plugin-prettier": "^4.2.1",
  
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5"
  }
}
